{
  "name": "com.example.hello",
  "displayName": "HelloWorld",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "watch": "chokidar \"./src/**/*.js\" \"./plugins/**/*\"  \"./www/index.html\" \"./www/css/*.css\" \"./node_modules/aframe/dist/aframe-master.*\" \"./node_modules/ar.js/aframe/build/aframe-ar.*\"  -c \"npm run rollup && cordova prepare\"",
    "rollup": "rollup -c"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "aframe": "^1.0.4",
    "cordova-ios": "^5.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "rollup-plugin-copy": "^3.3.0",
    "typescript": "^3.7.5"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios"
    ]
  }
}